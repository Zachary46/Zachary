<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NavigationView">
        <attr name="enablePadding" format="boolean"/>
        <attr name="forceSkipLoLLiPop" format="boolean"/>
        <attr name="drawPrimaryDark" format="boolean"/>
        <attr name="colorPrimaryDark" format="color"/>
        <attr name="colorStart" format="color"/>
        <attr name="colorEnd" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_border_overlay" format="boolean"/>
        <attr name="civ_fill_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="dashOrientation" format="integer"/>
    </declare-styleable>


    <declare-styleable name="BubbleView">
        <attr name="arrowWidth" format="dimension"/>
        <attr name="angle" format="dimension"/>
        <attr name="arrowHeight" format="dimension"/>
        <attr name="arrowPosition" format="dimension"/>
        <attr name="bubbleColor" format="color"/>
        <attr name="arrowCenter" format="boolean"/>
        <attr name="arrowLocation" format="enum">
            <enum name="left" value="0x00"/>
            <enum name="right" value="0x01"/>
            <enum name="top" value="0x02"/>
            <enum name="bottom" value="0x03"/>
        </attr>
    </declare-styleable>

</resources>